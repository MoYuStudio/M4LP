        <origin xyz="0 0 -0.09" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit effort="10" velocity="10" lower="-2.356" upper="2.356"/>
  </joint>

  <!-- 轮子：直径10cm，厚度4cm，黑色，垂直放置，紧贴小腿末端 -->
  <link name="RR_wheel">
    <visual>
      <geometry><cylinder length="0.04" radius="0.05"/></geometry>
      <material name="black"><color rgba="0 0 0 1"/></material>
      <origin xyz="0 0 0" rpy="0 0 0"/>
    </visual>
    <collision>
      <geometry><cylinder length="0.04" radius="0.05"/></geometry>
      <origin xyz="0 0 0" rpy="0 0 0"/>
    </collision>
    <inertial><mass value="0.08"/><inertia ixx="0.0001" iyy="0.0001" izz="0.0001"/></inertial>
  </link>
  <!-- 轮子连续关节：Z轴滚动，不限角度 -->
  <joint name="RR_wheel_joint" type="continuous">
    <parent link="RR_shank"/>
    <child  link="RR_wheel"/>
    <origin xyz="0 0 -0.25" rpy="1.57 0 0"/>
    <axis xyz="0 0 1"/>
    <limit effort="20" velocity="50"/>
  </joint>

  <!-- 陀螺仪传感器：安装在机器人中心，用于测量角速度 -->
  <link name="gyroscope">
    <inertial><mass value="0.01"/><inertia ixx="0.00001" iyy="0.00001" izz="0.00001"/></inertial>
  </link>
  <joint name="gyroscope_joint" type="fixed">
    <parent link="base"/>
    <child link="gyroscope"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </joint>

  <!-- 陀螺仪传感器定义 -->
  <gazebo reference="gyroscope">
    <sensor type="imu" name="gyroscope_sensor">
      <always_on>true</always_on>
      <update_rate>100</update_rate>
      <visualize>true</visualize>
      <imu>
        <angular_velocity>
          <x>
            <noise type="gaussian">
              <mean>0.0</mean>
              <stddev>0.01</stddev>
              <bias_mean>0.0</bias_mean>
              <bias_stddev>0.001</bias_stddev>
            </noise>
          </x>
          <y>
            <noise type="gaussian">
              <mean>0.0</mean>
              <stddev>0.01</stddev>
              <bias_mean>0.0</bias_mean>
              <bias_stddev>0.001</bias_stddev>
            </noise>
          </y>
          <z>
            <noise type="gaussian">
              <mean>0.0</mean>
              <stddev>0.01</stddev>
              <bias_mean>0.0</bias_mean>
              <bias_stddev>0.001</bias_stddev>
            </noise>
          </z>
        </angular_velocity>
        <linear_acceleration>
          <x>
            <noise type="gaussian">
              <mean>0.0</mean>
              <stddev>0.01</stddev>
              <bias_mean>0.0</bias_mean>
              <bias_stddev>0.001</bias_stddev>
            </noise>
          </x>
          <y>
            <noise type="gaussian">
              <mean>0.0</mean>
              <stddev>0.01</stddev>
              <bias_mean>0.0</bias_mean>
              <bias_stddev>0.001</bias_stddev>
            </noise>
          </y>
          <z>
            <noise type="gaussian">
              <mean>0.0</mean>
              <stddev>0.01</stddev>
              <bias_mean>0.0</bias_mean>
              <bias_stddev>0.001</bias_stddev>
            </noise>
          </z>
        </linear_acceleration>
      </imu>
    </sensor>
  </gazebo>

</robot>